%html
	%head
		%title PropMan
		= stylesheet_link_tag    'application', media: 'all'
		= javascript_include_tag 'application'
		= csrf_meta_tags

	%body

		#header
			= render 'layouts/header'

		#sidebar
			= render 'layouts/sidebar'

		#content
			.content-wrapper
				= render 'layouts/flash'
				= yield
			.clearfix


