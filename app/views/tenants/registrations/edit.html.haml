.row
	%div{class: "col-lg-12 heading"}
		%h1.page-header
			%i.ec-list
			Settings
.row
	.col-lg-12
		%div{class: "panel panel-default toggle"}
			.panel-heading
				%h3.panel-title

			.panel-body
				= form_for @tenant, url: tenant_registration_path, method: :put, html: {class: "form-horizontal group-border hover-stripped", role: "form"} do |f|
					.form-group
						= f.label :first_name, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.text_field :first_name, class: 'form-control', placeholder: 'Enter your first name'
					.form-group
						= f.label :last_name, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.text_field :last_name, class: 'form-control', placeholder: 'Enter your last name'
					.form-group
						= f.label :email, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.text_field :email, class: 'form-control', placeholder: 'Enter your email'
					.form-group
						= f.label :password, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.password_field :password, class: 'form-control', placeholder: 'Enter your password', autocomplete: "off"
					.form-group
						= f.label :password_confirmation, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.password_field :password_confirmation, class: 'form-control', placeholder: 'Enter your password', autocomplete: "off"
					.form-group
						= f.label :phone_number, class: "col-lg-2 col-md-2 col-sm-12 control-label"
						%div{class: "col-lg-10 col-md-10"}
							= f.text_field :phone_number, class: 'form-control', placeholder: 'Enter your phone number'
							%br
						%div{class: 'col-lg-6 col-md-6 col-sm-6 col-xs-12 text-right'}
							= f.submit 'Save changes', class: 'btn btn-primary'




